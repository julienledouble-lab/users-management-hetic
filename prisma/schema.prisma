generator client {
    provider = "prisma-client-js"
    output = "../generated/prisma"
}

datasource db {
    provider = "sqlite"
}

model User {
    id String @id @default(uuid())
    name String
    phone String?                 //? veut dire pas obligatoire
    email String @unique
    password String
    createdAt DateTime
    updatedAt DateTime
}